// Place your key bindings in this file to overwrite the defaults
[{
    "key": "cmd+e cmd+a",
    "command": "editor.emmet.action.wrapWithAbbreviation",
    "when": "editorTextFocus"
},
{
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
},
{
    "key": "cmd+b a",
    "command": "workbench.action.toggleActivityBarVisibility"
},
{
    "key": "cmd+b m",
    "command": "editor.action.toggleMinimap"
},
{
    "key": "cmd+b f",
    "command": "workbench.action.toggleFullScreen"
},
{
    "key": "ctrl+cmd+f",
    "command": "-workbench.action.toggleFullScreen"
},
{
    "key": "cmd+b t",
    "command": "workbench.action.terminal.toggleTerminal"
},
{
    "key": "ctrl+shift+[BracketRight]",
    "command": "-workbench.action.terminal.toggleTerminal"
},
{
    "key": "cmd+b j",
    "command": "workbench.action.togglePanel"
},
{
    "key": "cmd+j",
    "command": "-workbench.action.togglePanel"
},
{
    "key": "cmd+b o",
    "command": "workbench.action.output.toggleOutput"
},
{
    "key": "shift+cmd+u",
    "command": "-workbench.action.output.toggleOutput"
},
{
    "key": "cmd+b w",
    "command": "workbench.action.toggleTabsVisibility"
},
{
    "key": "ctrl+cmd+w",
    "command": "-workbench.action.toggleTabsVisibility"
},
{
    "key": "cmd+b s",
    "command": "workbench.action.toggleStatusbarVisibility"
},
{
    "key": "cmd+k q",
    "command": "workbench.action.quit"
},
{
    "key": "cmd+q",
    "command": "-workbench.action.quit"
},
{
    "key": "cmd+b b",
    "command": "workbench.action.toggleSidebarVisibility"
},
{
    "key": "cmd+b z",
    "command": "workbench.action.toggleZenMode"
},
{
    "key": "cmd+k z",
    "command": "-workbench.action.toggleZenMode"
},
{
    "key": "shift+cmd+f f",
    "command": "workbench.action.findInFiles"
},
{
    "key": "shift+cmd+f",
    "command": "-workbench.action.findInFiles"
},
{
    "key": "shift+cmd+f s",
    "command": "workbench.view.search",
    "when": "!searchViewletVisible"
},
{
    "key": "shift+cmd+f",
    "command": "-workbench.view.search",
    "when": "!searchViewletVisible"
},
{
    "key": "shift+cmd+f e",
    "command": "actions.findWithSelection"
},
{
    "key": "cmd+e",
    "command": "-actions.findWithSelection"
},
{
    "key": "cmd+e tab",
    "command": "editor.emmet.action.expandAbbreviation",
    "when": "config.emmet.triggerExpansionOnTab && editorTextFocus && !editorReadonly && !editorTabMovesFocus"
},
{
    "key": "cmd+e cmd+l",
    "command": "editor.emmet.action.wrapIndividualLinesWithAbbreviation"
},
{
    "key": "tab",
    "command": "-editor.emmet.action.expandAbbreviation",
    "when": "config.emmet.triggerExpansionOnTab && editorTextFocus && !editorReadonly && !editorTabMovesFocus"
},
{
    "key": "cmd+e u",
    "command": "editor.emmet.action.updateTag"
},
{
    "key": "cmd+e d",
    "command": "editor.emmet.action.removeTag"
},
{
    "key": "cmd+e n",
    "command": "editor.emmet.action.selectNextItem"
},
{
    "key": "cmd+e p",
    "command": "editor.emmet.action.selectPrevItem"
},
{
    "key": "cmd+e m",
    "command": "editor.emmet.action.mergeLines"
},
{
    "key": "cmd+e s",
    "command": "workbench.action.showEmmetCommands"
},
{
    "key": "cmd+k s",
    "command": "workbench.action.files.save"
},
{
    "key": "cmd+s",
    "command": "-workbench.action.files.save"
},
{
    "key": "cmd+k r",
    "command": "workbench.action.files.saveAs"
},
{
    "key": "shift+cmd+s",
    "command": "-workbench.action.files.saveAs"
},
{
    "key": "cmd+s a",
    "command": "workbench.action.files.saveAll"
},
{
    "key": "alt+cmd+s",
    "command": "-workbench.action.files.saveAll"
},
{
    "key": "cmd+k p",
    "command": "workbench.files.action.compareWithSaved"
},
{
    "key": "cmd+k d",
    "command": "-workbench.files.action.compareWithSaved"
},
{
    "key": "cmd+k ctrl+a",
    "command": "workbench.action.toggleAutoSave"
},
{
    "key": "cmd+r b",
    "command": "workbench.action.tasks.build"
},
{
    "key": "shift+cmd+b",
    "command": "-workbench.action.tasks.build"
},
{
    "key": "cmd+b shift+t",
    "command": "workbench.action.terminal.new"
},
{
    "key": "ctrl+[Quote]",
    "command": "-workbench.action.terminal.new"
},
{
    "key": "cmd+g n",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorFocus && !editorReadonly"
},
{
    "key": "f8",
    "command": "-editor.action.marker.nextInFiles",
    "when": "editorFocus && !editorReadonly"
},
{
    "key": "cmd+k n",
    "command": "workbench.action.files.newUntitledFile"
},
{
    "key": "cmd+n",
    "command": "-workbench.action.files.newUntitledFile"
},
{
    "key": "cmd+k cmd+n",
    "command": "workbench.action.newWindow"
},
{
    "key": "shift+cmd+n",
    "command": "-workbench.action.newWindow"
},
{
    "key": "cmd+k o",
    "command": "workbench.action.files.openFileFolder"
},
{
    "key": "cmd+o",
    "command": "-workbench.action.files.openFileFolder"
},
{
    "key": "cmd+k v",
    "command": "workbench.action.quickOpenView"
},
{
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenView"
},
{
    "key": "cmd+k right",
    "command": "workbench.action.nextEditor"
},
{
    "key": "alt+cmd+right",
    "command": "-workbench.action.nextEditor"
},
{
    "key": "cmd+k left",
    "command": "workbench.action.previousEditor"
},
{
    "key": "ctrl+shift+alt+cmd+8",
    "command": "-workbench.action.previousEditor"
},
{
    "key": "shift+cmd+w",
    "command": "-workbench.action.closeWindow"
},
{
    "key": "cmd+r r",
    "command": "code-runner.run"
},
{
    "key": "ctrl+alt+n",
    "command": "-code-runner.run"
},
{
    "key": "cmd+r k",
    "command": "code-runner.runCustomCommand"
},
{
    "key": "ctrl+alt+k",
    "command": "-code-runner.runCustomCommand"
},
{
    "key": "cmd+r s",
    "command": "code-runner.stop"
},
{
    "key": "ctrl+alt+m",
    "command": "-code-runner.stop"
},
{
    "key": "cmd+r l",
    "command": "code-runner.runByLanguage"
},
{
    "key": "ctrl+alt+j",
    "command": "-code-runner.runByLanguage"
},
{
    "key": "cmd+b v",
    "command": "toggleVim"
},
{
    "key": "cmd+m a",
    "command": "editor.action.addCommentLine",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "cmd+k cmd+c",
    "command": "-editor.action.addCommentLine",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "cmd+m b",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "shift+alt+a",
    "command": "-editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "cmd+m l",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "shift+cmd+7",
    "command": "-editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "cmd+e cmd+b",
    "command": "editor.emmet.action.toggleComment"
},
{
    "key": "cmd+m q",
    "command": "auto-comment-blocks.singleLineBlock",
    "when": "editorTextFocus"
},
{
    "key": "shift+enter",
    "command": "-auto-comment-blocks.singleLineBlock",
    "when": "editorTextFocus"
}
]